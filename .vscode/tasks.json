{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "task:build",
      "type": "shell",
      "command": "cargo build",
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "task:test",
      "type": "shell",
      "command": "cargo test",
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}
